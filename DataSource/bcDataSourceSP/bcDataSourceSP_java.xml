<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB_JDBC</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:JDBC"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />
    
    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>    

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tJDBCConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />
	
    <PARAMETER
      NAME="URL"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="URL"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"jdbc:"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="DRIVER_JAR"
      FIELD="TABLE"
      NUM_ROW="40"
      NB_LINES="2"
      REPOSITORY_VALUE="DRIVER_JAR"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      	<ITEMS> 
			<ITEM NAME="JAR_NAME" FIELD="MODULE_LIST"  /> 
		</ITEMS> 
    </PARAMETER>
	
    <PARAMETER
      NAME="DRIVER_CLASS"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="DRIVER_CLASS"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>JDBC</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="60"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="60"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="70"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SP_NAME"
      FIELD="TEXT"
      NUM_ROW="80"
      REQUIRED="true"
    >
      <DEFAULT>"myfunction"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="IS_FUNCTION"
      FIELD="CHECK"
      NUM_ROW="90"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="RETURN"
      FIELD="COLUMN_LIST"
      NUM_ROW="90"
      SHOW_IF="IS_FUNCTION == 'true'"
    >
    </PARAMETER>
    
	<PARAMETER
      NAME="SP_ARGS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="100"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="COLUMN" FIELD="COLUMN_LIST"/>
        
        <ITEM NAME="TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="IN">
            <ITEM NAME="IN" VALUE="IN"/>
            <ITEM NAME="OUT" VALUE="OUT"/>
            <ITEM NAME="INOUT" VALUE="INOUT"/>
            <ITEM NAME="RECORDSET" VALUE="RECORDSET"/>
          </ITEMS>
        </ITEM>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="7"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION></CODEGENERATION>
   
  <RETURNS/>
</COMPONENT>
